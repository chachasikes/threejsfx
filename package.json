{
  "name": "threejsfx",
  "version": "1.0.1",
  "description": "an effects library with a component document model",
  "private": true,
  "scripts": {
    "start": "http-server -c 0",
    "sync": "webpack;http-server -c 0&&browser-sync start --server --files 'src/*'",
    "build": "webpack --progress --color",
    "dev": "run-s dev:js webpack-server",
    "prebuild": "run-s prebuild build:dir build:js",
    "webpack-server": "webpack-dev-server --env.NODE_ENV=dev --progress --color --open --hot",
    "webpack-watch": "webpack --env.NODE_ENV=dev --progress --color --watch --hot",
    "dev:js": "webpack --env.NODE_ENV=dev"
  },

  "author": "me",
  "license": "BSD",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@huth/utils": "^1.0.1",
    "@wildpeaks/three-webpack-plugin": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "three": "^0.103.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {}
}
